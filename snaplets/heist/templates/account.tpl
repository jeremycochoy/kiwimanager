<apply template="_base">
  <table id="account_inf">
    <tr>
      <td>Login:</td>
      <td><userLogin /></td>
    </tr>
    <tr>
      <td>E-Mail:</td>
      <td><userEmail /></td>
    </tr>
    <tr>
      <td>First name:</td>
      <td><userFirstName /></td>
    </tr>
    <tr>
      <td>Last name:</td>
      <td><userLastName /></td>
    </tr>
    <tr>
      <td>Birthday:</td>
      <td><userBirthday /></td>
    </tr>
    <tr>
      <td>Last web connection:</td>
      <td><userLastLoginAt /></td>
    </tr>
    <tr>
      <td>Last in game connection:</td>
      <td><userLastIg /></td>
    </tr>
    <tr>
      <td>Last IP used:</td>
      <td><userLastLoginIp /></td>
    </tr>
    <tr>
      <td>Is your account banned:</td>
      <td><userBanned /></td>
    </tr>
    <tr>
      <td>Is your account verified:</td>
      <td><userVerified /></td>
    </tr>
  </table>
</apply>
